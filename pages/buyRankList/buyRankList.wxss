}

.qianSanMing{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  width: 96%;
  margin: 0 auto;
  align-items: flex-end;
}

.qsmCss {
  width: 32%;
  position: relative;
  text-align: center;
}

.qsmImage image{
  width: 137rpx;
  height: 137rpx;
  border-radius: 50%;
  border: #ccc 1px solid;
}

.qsmNum2{
  width: 100%;
  position: absolute;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: center;
}

.qsmNum2 view{
  width: 130rpx;
  height: 37rpx;
  line-height: 37rpx;
  background: #cccccc;
  color: #fff;
  border-radius: 20rpx;
}

.qsmNum2 view.one{
  height: 45rpx;
  line-height: 45rpx;
  background: #e8ba02;
}

.qsmNum2 view.thr{
  background: #cc6601;
}

.qsmFirst image{
  width: 170rpx;
  height: 170rpx;
}

.qsmFirst .qsmNum {
  top: 20rpx;
  left: 10rpx;
}

