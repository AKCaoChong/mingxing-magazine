<template name = "rightEmagListTemplate">
  <view class="emag-group">
    <view class="emag-main">
      <view class="emag-img" bindtap="onQiKanClick" data-kanid="{{id}}" data-thumlogo="{{thum_logo}}"><image src="{{thum_logo}}" mode="widthFix"></image></view>
      <text class="emag-title">{{title}}</text>
      <view class="emag-dy">
        <view class="f25" bindtap="onTypeClick" data-typeid="{{type}}" data-typename="{{typename}}">{{typename}}</view>
        <view class="f25"><text>{{buynum}}</text> 订阅</view>
      </view>
    </view>
    <view class="emag-number"><text>{{index+1}}</text></view>
  </view>
</template>