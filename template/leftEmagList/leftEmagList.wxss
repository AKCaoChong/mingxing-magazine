.leftEmag-group{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: flex-start;
}

.leftEmag-group .leftEmag-number{
  font-size: 32rpx;
  color: #353535;
  background: #e7e5e5;
  /* padding: 10rpx 20rpx; */
  text-align: center;
  width: 50rpx;
  height: 50rpx;
}

.leftEmag-group .leftEmag-main{
  display: flex;
  flex-direction: column;
  justify-content: center;
  text-align: center;
  margin-bottom: 30rpx;
  width: 80%;
}

.leftEmag-img {
  width: 100%;
  height: 405rpx; 
  overflow: hidden;
}

.leftEmag-img image{
  width: 100%;
  height: 405rpx; 
}

.leftEmag-title{
  padding: 10rpx 0 10rpx 0;
  align-self: flex-start;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: left;
}

.leftEmag-dy{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}