.emagListCss{
  display: flex;
  flex-direction: column;
  justify-content: center;
  text-align: center;
  margin-bottom: 30rpx;
}

.emagListImage{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 270rpx;
  height: 405rpx;
  overflow: hidden;
}

.emagListImage image{
  width: 270rpx;
  height: 405rpx;
}

.emagListTitle {
  padding: 10rpx 0 10rpx 0;
  align-self: flex-start;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: left;
}

.emagListDy{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}