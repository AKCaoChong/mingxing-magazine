.closePreviewcss{
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  width: 30rpx;
  height: 30rpx;
}

.closePreviewcss image{
  width: 37rpx;
}

.chosePreview{
  width: 80%;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  padding: 0rpx 0 80rpx 0;
  margin-top: 10rpx;
}

.yuedumatitle{
  padding: 20rpx 0;
}

.yuedumainner{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 100%;
}

.yuedumainpuNum{
  border: #d6d6d6 1px solid;
  border-radius: 8rpx;
  width: 60%;
  background: #f8f8f8;
  padding: 6rpx 10rpx;
}

.yuedumabuybtn{
  border: 0;
  color: #fff;
  height: 1.8rem;
  background: #c6343e;
  padding: 0;
  width: 30%;
  text-align: center;
  margin: 0;
  line-height: 1.8rem;
  font-size: 26rpx;
}