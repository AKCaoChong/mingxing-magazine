.rank-content{
  display: flex;
  flex-flow: column;
  width: 90%;
  padding: 3% 2%;
  margin: 0 auto;
  /* border-top:  */
  line-height: 50rpx;
  color: #919191;
  text-align: justify;
}

.phbbg{
  background: #f7f8fa;
